{
  "name": "regent",
  "version": "4.0.0",
  "description": "Javascript rules engine",
  "repository": {
    "type": "git",
    "url": "https://github.com/northwesternmutual/regent"
  },
  "source": "src/regent.ts",
  "main": "dist/regent.js",
  "module": "dist/regent.modern.module.js",
  "unpkg": "dist/regent.umd.js",
  "ts-standard": {
    "project": "./tsconfig.json"
  },
  "scripts": {
    "test": "jest",
    "test-watch": "jest --watch",
    "build": "microbundle --external none",
    "dev": "microbundle watch",
    "stryker": "npx stryker run",
    "publish": "npm run build && npm publish",
    "lint": "eslint src/**/*.ts",
    "sanity": "npx tsc example/sanity-test.ts && node example/sanity-test.js"
  },
  "pre-push": [
    "test",
    "build"
  ],
  "keywords": [
    "logic",
    "engine",
    "rules",
    "self-documenting",
    "business"
  ],
  "author": "Northwestern Mutual",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/northwesternmutual/regent/issues"
  },
  "homepage": "https://github.com/northwesternmutual/regent/blob/master/README.md",
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.8.6",
    "@babel/preset-env": "^7.8.6",
    "@babel/preset-typescript": "^7.18.6",
    "@stryker-mutator/core": "^3.0.0",
    "@stryker-mutator/html-reporter": "^3.0.0",
    "@stryker-mutator/javascript-mutator": "^3.0.0",
    "@stryker-mutator/jest-runner": "^3.0.0",
    "@stryker-mutator/typescript": "^4.0.0",
    "@stryker-mutator/typescript-checker": "^6.2.2",
    "@types/jest": "^29.0.3",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "5.38.0",
    "eslint": "^8.23.1",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.3.0",
    "eslint-plugin-promise": "^6.0.1",
    "jest": "^26.6.3",
    "microbundle": "^0.13.1",
    "pre-push": "^0.1.4",
    "standard": "^14.3.1",
    "ts-loader": "^9.4.1",
    "ts-standard": "^11.0.0",
    "typescript": "^4.8.3"
  },
  "dependencies": {},
  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  }
}
